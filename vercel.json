{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist/sakai-ng/browser",
    "installCommand": "npm install",
    "devCommand": "ng serve --port $PORT",
    "framework": "angular",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}